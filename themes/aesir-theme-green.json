  "tokenColors": [
    {
      "settings": {
        "background": "#1F2228",
        "foreground": "#D4D4D4",
        "selectionBorder": "#8D92A0",
        "findHighlight": "#272b33",
        "findHighlightForeground": "#D4D4D4",
        "activeGuide": "#C0392B",
        "bracketsForeground": "#5D6879",
        "bracketsOptions": "stippled_underline",
        "bracketsContentsForeground": "#D4D4D47F",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#5d6879",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#6b788e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#D4D4D4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "attributes",
      "scope": "entity.attribute",
      "settings": {
        "foreground": "#6b788e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [ "keyword.operator" ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6b788e"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#E74C3C",
        "foreground": "#333"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#6b788e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#6b788e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#5D6879",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#27ae60"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#27ae60"
      }
    },
    {
      "name": "Functions",
      "scope": [ "entity.name.function", "support.function" ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [ "meta.tag entity.other.attribute-name", "entity.other.attribute-name.html" ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6b788e"
      }
    },
    {
      "name": "Classes",
      "scope": [ "entity.name.type", "entity.other.inherited-class", "support.class" ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": [ "meta.property-name", "support.type.property-name" ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Markup Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#2ECC71"
      }
    },
    {
      "name": "Markup Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#E74C3C"
      }
    },
    {
      "name": "Markup Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#5D6879"
      }
    },
    {
      "name": "Markup Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#5D6879"
      }
    },
    {
      "name": "Markup Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Underline Link",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#27AE60"
      }
    },
    {
      "name": "Markup List",
      "scope": "markup.list",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Markup Raw",
      "scope": "markup.raw",
      "settings": {
        "foreground": "#5D6879"
      }
    },
    {
      "name": "[HTML] - Entity Other",
      "scope": "text.html.basic entity.other",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5D6879"
      }
    }
  ]
}
